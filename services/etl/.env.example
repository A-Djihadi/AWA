# ETL Environment Configuration Template

# Database Configuration
SUPABASE_URL=your_supabase_project_url
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# ETL Processing Configuration
ETL_BATCH_SIZE=100
ETL_MAX_WORKERS=4
ETL_MIN_QUALITY=0.6

# Directory Configuration
ETL_SOURCE_DIR=/app/data/raw
ETL_PROCESSED_DIR=/app/data/processed

# Logging Configuration
ETL_LOG_LEVEL=INFO
ETL_LOG_FILE=/app/logs/etl.log

# Quality Control
ETL_ENABLE_QUALITY_CHECKS=true
ETL_STRICT_VALIDATION=false

# Performance Settings
ETL_ENABLE_CACHING=true
ETL_CACHE_TTL=3600

# Monitoring and Alerts
ETL_ENABLE_METRICS=true
ETL_ALERT_ON_FAILURE=true
ETL_ALERT_EMAIL=admin@yourcompany.com
